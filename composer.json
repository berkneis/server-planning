{
  "name": "berkneis/server-planning",
  "description": "The server is a simple bundle that checks whether the virtual machine meets the need.",
  "keywords": ["symfony"],
  "type": "symfony-bundle",
  "require": {
    "php": ">=7.1.3",
    "symfony/dependency-injection": "~4.0",
    "symfony/http-kernel": "~4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0",
    "symfony/framework-bundle": "~4.0",
    "phpunit/phpunit": "^5.7|^6.0",
    "symfony/yaml": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "BerkNeis\\ServerPlanning\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BerkNeis\\ServerPlanning\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Berk",
      "email": "neisberk@gmail.com"
    }
  ]
}
